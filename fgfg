local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local player = Players.LocalPlayer
local placeId = game.PlaceId

-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –¥–∞–Ω–Ω—ã–µ –≤ –≥–ª–æ–±–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è –∞–≤—Ç–æ-—Ä–µ—Å—Ç–∞—Ä—Ç–∞
_G.AutoSpamActive = true

local messages = {
    "–ü—Ä–æ–¥–∞–º Godly –¥–µ—à–µ–≤–æ –∑–∞ –≥—Ä–Ω. –ü–∏—à–∏: Hexon_Shop",
    "Godly –∑–∞ –≥—Ä–∏–≤–Ω—ã. –ë—ã—Å—Ç—Ä–æ. Hexon_Shop",
    "–ö—É–ø–∏—Ç—å Godly –Ω–µ–¥–æ—Ä–æ–≥–æ. –û–ø–ª–∞—Ç–∞ –≥—Ä–Ω. Hexon_Shop",
    "–ï—Å—Ç—å Godly –ø–æ –≤—ã–≥–æ–¥–Ω–æ–π —Ü–µ–Ω–µ –≤ –≥—Ä–Ω. Hexon_Shop",
    "Godly –¥–ª—è —Ç—Ä–µ–π–¥–∞ –∑–∞ –≥—Ä–∏–≤–Ω—ã. Hexon_Shop",
    "–î–µ—à–µ–≤—ã–π Godly –∑–∞ –≥—Ä–Ω. –ü–∏—à–∏: Hexon_Shop",
    "–ü—Ä–æ–¥–∞–∂–∞ Godly. –û–ø–ª–∞—Ç–∞ –≤ –≥—Ä–∏–≤–Ω–∞—Ö. Hexon_Shop",
    "Godly –≤ –Ω–∞–ª–∏—á–∏–∏. –¶–µ–Ω–∞ –≤ –≥—Ä–Ω. Hexon_Shop",
    "–ü–æ–∫—É–ø–∞–π Godly –∑–∞ –≥—Ä–∏–≤–Ω—ã. Hexon_Shop",
    "Godly –ø–æ –Ω–∏–∑–∫–æ–π —Ü–µ–Ω–µ –≤ –≥—Ä–Ω. Hexon_Shop"
}

-- –°—Ç–∏–∫–µ—Ä—ã/—Å–º–∞–π–ª–∏–∫–∏
local stickers = {"üíé", "üî•", "üéÆ", "üí∞", "üó°Ô∏è", "‚≠ê", "üîπ", "üõí", "üéØ", "‚ö°"}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–º–µ—à–∏–≤–∞–Ω–∏—è
local function shuffle(tbl)
    local copy = {}
    for i, v in ipairs(tbl) do
        copy[i] = v
    end
    for i = #copy, 2, -1 do
        local j = math.random(i)
        copy[i], copy[j] = copy[j], copy[i]
    end
    return copy
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Å–ª—É—á–∞–π–Ω–æ–≥–æ —Å—Ç–∏–∫–µ—Ä–∞
local function getRandomSticker()
    return stickers[math.random(1, #stickers)]
end

-- –§—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –≤ —á–∞—Ç
local function sendMessage(text)
    local decoratedText = getRandomSticker() .. " " .. text .. " " .. getRandomSticker()
    
    -- –ü—Ä–æ–±—É–µ–º –≤—Å–µ –º–µ—Ç–æ–¥—ã –æ—Ç–ø—Ä–∞–≤–∫–∏
    local success = false
    
    -- –ú–µ—Ç–æ–¥ 1: –°—Ç–∞—Ä—ã–π —á–∞—Ç
    local function tryOldChat()
        local success1, err1 = pcall(function()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(decoratedText, "All")
        end)
        return success1
    end
    
    -- –ú–µ—Ç–æ–¥ 2: –ù–æ–≤—ã–π —á–∞—Ç
    local function tryNewChat()
        local success2, err2 = pcall(function()
            game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(decoratedText)
        end)
        return success2
    end
    
    -- –ú–µ—Ç–æ–¥ 3: –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –ø—É—Ç—å –∫ –Ω–æ–≤–æ–º—É —á–∞—Ç—É
    local function tryNewChatAlt()
        local success3, err3 = pcall(function()
            local tcs = game:GetService("TextChatService")
            if tcs:FindFirstChild("TextChannels") then
                local general = tcs.TextChannels:FindFirstChild("RBXGeneral") or tcs.TextChannels:FindFirstChild("General")
                if general then
                    general:SendAsync(decoratedText)
                end
            end
        end)
        return success3
    end
    
    success = tryOldChat() or tryNewChat() or tryNewChatAlt()
    
    if success then
        print("‚úì –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ: " .. decoratedText)
    else
        print("‚úó –ù–µ —É–¥–∞–ª–æ—Å—å –æ—Ç–ø—Ä–∞–≤–∏—Ç—å")
    end
    
    return success
end

-- –û–∂–∏–¥–∞–Ω–∏–µ –∑–∞–≥—Ä—É–∑–∫–∏ —á–∞—Ç–∞
local function waitForChat()
    local maxWait = 10
    local waited = 0
    
    while waited < maxWait do
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å—Ç–∞—Ä—ã–π —á–∞—Ç
        local oldChatExists = pcall(function()
            return game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents ~= nil
        end)
        
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–æ–≤—ã–π —á–∞—Ç
        local newChatExists = pcall(function()
            return game:GetService("TextChatService").TextChannels ~= nil
        end)
        
        if oldChatExists or newChatExists then
            return true
        end
        
        waited = waited + 1
        task.wait(1)
    end
    
    return false
end

-- –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–∫—Ä–∏–ø—Ç–∞
local function startSpamming()
    print("üöÄ –ó–∞–ø—É—Å–∫ –∞–≤—Ç–æ-—Å–ø–∞–º–∞...")
    
    -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏ –∏–≥—Ä—ã
    task.wait(5)
    
    local messageCount = 0
    
    while _G.AutoSpamActive do
        -- –ñ–¥–µ–º –¥–æ—Å—Ç—É–ø–Ω–æ—Å—Ç–∏ —á–∞—Ç–∞
        if waitForChat() then
            local shuffled = shuffle(messages)
            messageCount = 0
            
            -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º 5 —Å–æ–æ–±—â–µ–Ω–∏–π
            for i = 1, 5 do
                if not _G.AutoSpamActive then break end
                
                local sent = sendMessage(shuffled[i])
                if sent then
                    messageCount = messageCount + 1
                end
                
                -- –°–ª—É—á–∞–π–Ω–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏
                local delay = math.random(3, 4)
                for t = 1, delay do
                    if not _G.AutoSpamActive then break end
                    task.wait(1)
                end
            end
            
            -- –ü–µ—Ä–µ–∑–∞—Ö–æ–¥–∏–º –ø–æ—Å–ª–µ 5 —Å–æ–æ–±—â–µ–Ω–∏–π
            if _G.AutoSpamActive and messageCount >= 3 then
                print("üîÑ –ü–µ—Ä–µ–∑–∞—Ö–æ–¥ –Ω–∞ –Ω–æ–≤—ã–π —Å–µ—Ä–≤–µ—Ä...")
                
                -- –ñ–¥–µ–º –Ω–µ–º–Ω–æ–≥–æ –ø–µ—Ä–µ–¥ –ø–µ—Ä–µ–∑–∞—Ö–æ–¥–æ–º
                task.wait(2)
                
                -- –ü–µ—Ä–µ–∑–∞—Ö–æ–¥–∏–º
                local success, error = pcall(function()
                    TeleportService:Teleport(placeId)
                end)
                
                if not success then
                    print("‚ö†Ô∏è –û—à–∏–±–∫–∞ –ø—Ä–∏ –ø–µ—Ä–µ–∑–∞—Ö–æ–¥–µ: " .. tostring(error))
                    task.wait(5)
                else
                    -- –ñ–¥–µ–º —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é
                    task.wait(10)
                end
            else
                -- –ï—Å–ª–∏ –æ—Ç–ø—Ä–∞–≤–∏–ª–∏ –º–∞–ª–æ —Å–æ–æ–±—â–µ–Ω–∏–π, –∂–¥–µ–º –∏ –ø—Ä–æ–¥–æ–ª–∂–∞–µ–º
                task.wait(10)
            end
        else
            print("‚è≥ –ß–∞—Ç –Ω–µ –∑–∞–≥—Ä—É–∑–∏–ª—Å—è, –∂–¥—É...")
            task.wait(5)
        end
    end
end

-- –ê–≤—Ç–æ-—Ä–µ—Å—Ç–∞—Ä—Ç –ø—Ä–∏ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
local function setupAutoRestart()
    player.OnTeleport:Connect(function(state)
        if state == Enum.TeleportState.Started then
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–∫—Ä–∏–ø—Ç –¥–ª—è –∞–≤—Ç–æ-–∑–∞–ø—É—Å–∫–∞
            local scriptCode = [[
                task.wait(5)
                if _G.AutoSpamActive == nil then
                    _G.AutoSpamActive = true
                end
                
                local Players = game:GetService("Players")
                local TeleportService = game:GetService("TeleportService")
                
                -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏
                task.wait(8)
                
                -- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–ø–∞–º
                while _G.AutoSpamActive do
                    local success, err = pcall(function()
                        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —á–∞—Ç
                        local chatLoaded = false
                        for i = 1, 10 do
                            local oldChat = pcall(function() return game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents end)
                            local newChat = pcall(function() return game:GetService("TextChatService").TextChannels end)
                            if oldChat or newChat then
                                chatLoaded = true
                                break
                            end
                            task.wait(1)
                        end
                        
                        if chatLoaded then
                            -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Å–æ–æ–±—â–µ–Ω–∏—è
                            local messages = {
                                "–ü—Ä–æ–¥–∞–º Godly –¥–µ—à–µ–≤–æ –∑–∞ –≥—Ä–Ω. –ü–∏—à–∏: Hexon_Shop",
                                "Godly –∑–∞ –≥—Ä–∏–≤–Ω—ã. –ë—ã—Å—Ç—Ä–æ. Hexon_Shop",
                                "–ö—É–ø–∏—Ç—å Godly –Ω–µ–¥–æ—Ä–æ–≥–æ. –û–ø–ª–∞—Ç–∞ –≥—Ä–Ω. Hexon_Shop",
                                "–ï—Å—Ç—å Godly –ø–æ –≤—ã–≥–æ–¥–Ω–æ–π —Ü–µ–Ω–µ –≤ –≥—Ä–Ω. Hexon_Shop",
                                "Godly –¥–ª—è —Ç—Ä–µ–π–¥–∞ –∑–∞ –≥—Ä–∏–≤–Ω—ã. Hexon_Shop",
                                "–î–µ—à–µ–≤—ã–π Godly –∑–∞ –≥—Ä–Ω. –ü–∏—à–∏: Hexon_Shop",
                                "–ü—Ä–æ–¥–∞–∂–∞ Godly. –û–ø–ª–∞—Ç–∞ –≤ –≥—Ä–∏–≤–Ω–∞—Ö. Hexon_Shop",
                                "Godly –≤ –Ω–∞–ª–∏—á–∏–∏. –¶–µ–Ω–∞ –≤ –≥—Ä–Ω. Hexon_Shop",
                                "–ü–æ–∫—É–ø–∞–π Godly –∑–∞ –≥—Ä–∏–≤–Ω—ã. Hexon_Shop",
                                "Godly –ø–æ –Ω–∏–∑–∫–æ–π —Ü–µ–Ω–µ –≤ –≥—Ä–Ω. Hexon_Shop"
                            }
                            
                            local stickers = {"üíé", "üî•", "üéÆ", "üí∞", "üó°Ô∏è", "‚≠ê", "üîπ", "üõí", "üéØ", "‚ö°"}
                            
                            for msgIndex = 1, 5 do
                                if not _G.AutoSpamActive then break end
                                
                                local msg = messages[math.random(1, #messages)]
                                local sticker1 = stickers[math.random(1, #stickers)]
                                local sticker2 = stickers[math.random(1, #stickers)]
                                local fullMsg = sticker1 .. " " .. msg .. " " .. sticker2
                                
                                -- –ü—Ä–æ–±—É–µ–º –æ—Ç–ø—Ä–∞–≤–∏—Ç—å
                                pcall(function()
                                    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(fullMsg, "All")
                                end)
                                
                                pcall(function()
                                    game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(fullMsg)
                                end)
                                
                                task.wait(math.random(3, 5))
                            end
                            
                            -- –ü–µ—Ä–µ–∑–∞—Ö–æ–¥–∏–º
                            task.wait(2)
                            TeleportService:Teleport(game.PlaceId)
                        end
                    end)
                    
                    if not success then
                        task.wait(5)
                    end
                    task.wait(1)
                end
            ]]
            
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏ –∑–∞–ø—É—Å–∫–∞–µ–º –ø–æ—Å–ª–µ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
            TeleportService:SetTeleportGui(Instance.new("ScreenGui"))
            TeleportService:SetTeleportData(scriptCode)
        end
    end)
end

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
print("==================================")
print("ü§ñ –ê–í–¢–û-–°–ü–ê–ú –°–ö–†–ò–ü–¢ –ê–ö–¢–ò–í–ò–†–û–í–ê–ù")
print("ü§ñ Godly –∑–∞ –≥—Ä–∏–≤–Ω—ã | Hexon_Shop")
print("==================================")

-- –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –∞–≤—Ç–æ-—Ä–µ—Å—Ç–∞—Ä—Ç
setupAutoRestart()

-- –ó–∞–ø—É—Å–∫–∞–µ–º –æ—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª
task.wait(3)
startSpamming()

-- –§—É–Ω–∫—Ü–∏—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∏ (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)
local function stopScript()
    _G.AutoSpamActive = false
    print("‚úã –°–∫—Ä–∏–ø—Ç –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
end

-- –≠–∫—Å–ø–æ—Ä—Ç–∏—Ä—É–µ–º —Ñ—É–Ω–∫—Ü–∏—é –æ—Å—Ç–∞–Ω–æ–≤–∫–∏
_G.StopAutoSpam = stopScript

print("üìù –ò—Å–ø–æ–ª—å–∑—É–π StopAutoSpam() –¥–ª—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∏")
